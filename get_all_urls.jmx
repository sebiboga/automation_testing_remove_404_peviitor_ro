            <stringProp name="script">def numFoundStr = vars.get(&quot;numFound&quot;)
int numFound = 0
if (numFoundStr != null &amp;&amp; !numFoundStr.trim().isEmpty() &amp;&amp; numFoundStr.isInteger()) {
    numFound = numFoundStr.toInteger()
}
int loopCount = (numFound / 30) + 1
vars.put(&quot;loopCount&quot;, loopCount.toString())
log.info(&quot;Loop count for Loop Controller set to: &quot; + loopCount)
</stringProp>
