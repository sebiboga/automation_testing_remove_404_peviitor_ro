          <stringProp name="parameters"></stringProp>
          <stringProp name="script">// Assume &quot;rc&quot; is a JMeter variable holding the response code as a string
def rc = vars.get(&quot;rc&quot;)
if (rc == &apos;404&apos;) {
   

    // Get the URL to delete from a JMeter variable named &quot;url&quot;
    def urlToDelete = vars.get(&quot;url&quot;)  
    if (urlToDelete == null || urlToDelete.trim().isEmpty()) {
        log.error(&quot;No URL provided in JMeter variable &apos;url&apos;. Aborting deletion.&quot;)
        return
    }

    def core = &apos;jobs&apos;
    def solrUrl = &quot;http://zimbor.go.ro/solr/${core}/update?commit=true&quot;
    def userToken = &quot;sebi:sebiisTheBest0102&quot;  // Your basic auth credentials

    def deleteXml = &quot;&lt;delete&gt;&lt;query&gt;job_link:\&quot;${urlToDelete}\&quot;&lt;/query&gt;&lt;/delete&gt;&quot;

    def curlCommand = [
        &apos;curl&apos;,
        &apos;-u&apos;, userToken,
        solrUrl,
        &apos;-H&apos;, &apos;Content-Type: text/xml&apos;,
        &apos;--data-binary&apos;, deleteXml
    ]

    def process = curlCommand.execute()

    def output = new StringWriter()
    def error = new StringWriter()
    process.waitForProcessOutput(output, error)
    int exitCode = process.exitValue()

   
    if (exitCode != 0) {
        log.error(&quot;Curl command failed with exit code ${exitCode}&quot;)
        log.error(&quot;Error output:\n${error.toString()}&quot;)
    }
} else {
    
}</stringProp>
